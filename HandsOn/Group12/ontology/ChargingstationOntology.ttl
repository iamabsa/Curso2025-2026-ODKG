			mod:createdWith <https://chowlk.linkeddata.es/> .

#################################
#    Object Properties
#################################

## ns:isInDistrict
ns:isInDistrict rdf:type owl:ObjectProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range ns:District ;
		rdfs:label "is in district" .

## ns:districtCity
ns:districtCity rdf:type owl:ObjectProperty ;
		rdfs:domain ns:District ;
		rdfs:range ns:City ;
		rdfs:label "district city" .

## ns:isOperatedBy
ns:isOperatedBy rdf:type owl:ObjectProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range ns:Operator ;
		rdfs:label "is operated by" .

## ns:isCityOf
ns:isCityOf rdf:type owl:ObjectProperty ;
		rdfs:domain ns:City ;
		rdfs:range ns:Country ;
		rdfs:label "is city of" .

#################################
#    Data Properties
#################################

## ns:countryCode
ns:countryCode rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:Country ;
		rdfs:range xsd:integer ;
		rdfs:label "country code" .

## ns:directPayment
ns:directPayment rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range xsd:integer ;
		rdfs:label "direct payment" .

## ns:hasID
ns:hasID rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range xsd:string ;
		rdfs:label "has i d" .

## ns:hasAddress
ns:hasAddress rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range xsd:string ;
		rdfs:label "has address" .

## ns:hasEuId
ns:hasEuId rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range xsd:string ;
		rdfs:label "has eu id" .

## ns:geoLoc
ns:geoLoc rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range xsd:string ;
		rdfs:label "geo loc" .

## ns:hasID
ns:hasID rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range xsd:string ;
		rdfs:label "has i d" .

