@prefix rr:   <http://www.w3.org/ns/r2rml#> .
@prefix rml:  <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:   <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .

@prefix :   <https://iamabsa.github.io/chargingstation/resource/> .
@prefix ns: <https://iamabsa.github.io/chargingstation#> .

@base <https://iamabsa.github.io/chargingstation#> .

#######################################################################
# TriplesMap: Charging Station
#######################################################################

<#TriplesMap_Station> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "CSV/ELADESTELLEOGD-with-links.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "https://iamabsa.github.io/chargingstation/resource/station/{EVSEID}" ;
    rr:class ns:ChargingStation
  ] ;

  # Literals / attributes
  rr:predicateObjectMap [
    rr:predicate rdfs:label ;
    rr:objectMap [ rml:reference "DESIGNATION" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ns:evseId ;
    rr:objectMap [ rml:reference "EVSEID" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ns:address ;
    rr:objectMap [ rml:reference "ADDRESS" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ns:countryCode ;
    rr:objectMap [ rml:reference "COUNTRYCODE" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ns:directPayment ;
    rr:objectMap [ rml:reference "DIRECTPAYMENT" ; rr:datatype xsd:boolean ]
  ] ;

  # Link to District
  rr:predicateObjectMap [
    rr:predicate ns:isInDistrict ;
    rr:objectMap [
      rr:template "https://iamabsa.github.io/chargingstation/resource/district/{CITY}-{BEZIRK}" ;
      rr:termType rr:IRI
    ]
  ] ;

  # GeoSPARQL link: station -> geometry node
  rr:predicateObjectMap [
    rr:predicate geo:hasGeometry ;
    rr:objectMap [
      rr:template "https://iamabsa.github.io/chargingstation/resource/geometry/{EVSEID}" ;
      rr:termType rr:IRI
    ]
  ] ;

  # Wikidata links for station
  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "city_URL" ; rr:termType rr:IRI ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "Bezirk_URL" ; rr:termType rr:IRI ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "Operator_URL" ; rr:termType rr:IRI ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "COUNTRYCODE_URL" ; rr:termType rr:IRI ]
  ] .

#######################################################################
# TriplesMap: Geometry
#######################################################################

<#TriplesMap_Geometry> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "CSV/ELADESTELLEOGD-with-links.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;
  rr:subjectMap [
    rr:template "https://iamabsa.github.io/chargingstation/resource/geometry/{EVSEID}" ;
    rr:class geo:Geometry
  ] ;
  rr:predicateObjectMap [
    rr:predicate geo:asWKT ;
    rr:objectMap [
      rml:reference "SHAPE" ;
      rr:datatype geo:wktLiteral
    ]
  ] .

#######################################################################
# TriplesMap: Operator
#######################################################################

<#TriplesMap_Operator> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "CSV/ELADESTELLEOGD-with-links.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "https://iamabsa.github.io/chargingstation/resource/{OPERATORNAME}" ;
    rr:class ns:Operator
  ] ;

  rr:predicateObjectMap [
    rr:predicate rdfs:label ;
    rr:objectMap [ rml:reference "OPERATORNAME" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "Operator_URL" ; rr:termType rr:IRI ]
  ] .

#######################################################################
# TriplesMap: District
#######################################################################

<#TriplesMap_District> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source "CSV/ELADESTELLEOGD-with-links.csv" ;
    rml:referenceFormulation ql:CSV
  ] ;

  rr:subjectMap [
    rr:template "https://iamabsa.github.io/chargingstation/resource/{BEZIRK}" ;
    rr:class ns:District
  ] ;
  
  rr:predicateObjectMap [
    rr:predicate rdfs:label ;
    rr:objectMap [ rml:reference "BEZIRK" ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate ns:districtCity ;
    rr:objectMap [
      rr:template "https://iamabsa.github.io/chargingstation/resource/{CITY}" ;
      rr:termType rr:IRI
    ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate owl:sameAs ;
    rr:objectMap [ rml:reference "Bezirk_URL" ; rr:termType rr:IRI ]
  ] .

